{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"    
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-shadow" : "off",
        "no-use-before-define" : "off",
        "no-console" : "off",
        "no-param-reassign" :"off",
        "no-restricted-syntax" : "off",
        "no-unused-vars" : "off",
        "no-plusplus" : "off",
        "no-else-return" : "off"
    }
}
